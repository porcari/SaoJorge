      </FIELDS>
      <ACTIONSCRIPT>#CHECK("SELECT COUNT(EBL_CODIGO) AS N FROM EBL_INTEGRACOES WHERE EBL_CODIGO=:CODIGO", N>0, const.COD_DUPL);

INSERT INTO EBL_INTEGRACOES(EBL_CODIGO,EBL_DESCRICAO,EBL_TOKEN,EBL_URL,EBL_MODELO,EBL_TRANS_ID)
                    VALUES (:CODIGO,:DESCRICAO,:TOKEN,:URL,:MODELO,:TRANS_ID)
#RETURN (EBL_INTEGRACAO);
</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
