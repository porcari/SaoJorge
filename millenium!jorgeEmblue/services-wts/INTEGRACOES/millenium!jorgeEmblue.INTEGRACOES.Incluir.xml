<?xml version="1.0"?>
<root>
  <OBJECT NAME="INTEGRACOES">
    <METHOD NAME="Incluir" DESCRIPTION="Incluir" VERSION="22" INTFTYPE="1" THREADSAFE="0">
      <PARAMS>
        <GROUPS>
          <GROUP/>
          <GROUP NAME="Web Service"/>
          <GROUP NAME="Configura&#231;&#245;es"/>
        </GROUPS>
        <PARAM NAME="CODIGO" SIZE="10" FLAGS="17" PROJECTION="0" ORDER="1" FIELDLABEL="C&#243;digo" CTAB="0"/>
        <PARAM NAME="DESCRICAO" SIZE="60" FLAGS="17" PROJECTION="0" ORDER="2" FIELDLABEL="Descri&#231;&#227;o" CTAB="0"/>
        <PARAM NAME="TOKEN" SIZE="250" FLAGS="2065" PROJECTION="0" ORDER="5" FIELDLABEL="Token de Acesso" GROUPNAME="Web Service" CTAB="0"/>
        <PARAM NAME="URL" SIZE="250" FLAGS="2065" PROJECTION="0" ORDER="4" FIELDLABEL="Url" GROUPNAME="Web Service" CTAB="0"/>
        <PARAM NAME="MODELO" SIZE="50" FLAGS="17" PROJECTION="0" ORDER="7" LOOKUP="list:'CLIENTES','Envio de Clientes';" FIELDLABEL="Modelo de Integra&#231;&#227;o" GROUPNAME="Configura&#231;&#245;es" CTAB="0"/>
        <PARAM NAME="TRANS_ID" FORMAT="N" SIZE="8" FLAGS="17" DEFAULT="0" PROJECTION="0" ORDER="8" FIELDLABEL="Trans Id" GROUPNAME="Configura&#231;&#245;es" CTAB="0"/>
      </PARAMS>
      <FIELDS>
