<?xml version="1.0"?>
<root>
  <OBJECT NAME="INTEGRACOES">
    <METHOD NAME="ListarItensErro" DESCRIPTION="Itens com Erro" VERSION="27" THREADSAFE="0">
      <PARAMS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <PARAM NAME="INTEGRACAO" FORMAT="N" SIZE="8" FLAGS="3" DEFAULT="MILLENIUM!JORGEEMBLUE.INTEGRACOES.LISTAR([],'INTEGRACAO',True)" PROJECTION="0" ORDER="1" LOOKUP="INTEGRACOES.Listar" LOOKUPKEY="INTEGRACAO" LOOKUPDISPLAY="DESCRICAO" LOOKUPCODE="CODIGO" FIELDLABEL="Integra&#231;&#227;o" CTAB="0"/>
        <PARAM NAME="STATUS" SIZE="25" FLAGS="1" DEFAULT="REPROCESSANDO" PROJECTION="0" ORDER="2" LOOKUP="list:'AG_LIBERACAO','Bloqueado';'REPROCESSANDO','Temtando integrar';'NAO_INTEGRAR','N&#227;o integrar';" FIELDLABEL="Status" CTAB="0"/>
        <PARAM NAME="OBS" SIZE="250" FLAGS="1" PROJECTION="0" ORDER="4" FIELDLABEL="Observa&#231;&#227;o" CTAB="0"/>
        <PARAM NAME="CODIGO" SIZE="250" FLAGS="1" PROJECTION="0" ORDER="3" FIELDLABEL="C&#243;digo" CTAB="0"/>
      </PARAMS>
      <FIELDS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <FIELD NAME="ITEM_ERRO" FORMAT="N" SIZE="8" PROJECTION="0" FIELDLABEL="Codigo" CTAB="0"/>
        <FIELD NAME="INTEGRACAO" FORMAT="N" SIZE="8" PROJECTION="0" FIELDLABEL="Integracao" CTAB="0"/>
        <FIELD NAME="CODIGO" SIZE="250" FLAGS="1" PROJECTION="0" ORDER="6" FIELDLABEL="C&#243;digo" CTAB="0"/>
        <FIELD NAME="STATUS" SIZE="25" FLAGS="1" PROJECTION="0" ORDER="9" FIELDLABEL="Status" CTAB="0"/>
        <FIELD NAME="DATA" FORMAT="H" SIZE="18" FLAGS="1" PROJECTION="0" ORDER="8" FIELDLABEL="Data" CTAB="0"/>
        <FIELD NAME="TENTATIVAS" FORMAT="N" SIZE="8" FLAGS="1" PROJECTION="0" ORDER="10" FIELDLABEL="Tentativas" CTAB="0"/>
        <FIELD NAME="OBS" SIZE="250" FLAGS="1" PROJECTION="0" ORDER="7" FIELDLABEL="Obs" CTAB="0"/>
      </FIELDS>
      <ACTIONSCRIPT>SELECT E.EBL_ITEM_ERRO  AS ITEM_ERRO,
       E.EBL_INTEGRACAO AS INTEGRACAO,
       E.EBL_CODIGO AS CODIGO,
       IIF(E.EBL_STATUS="AG_LIBERACAO","Bloqueado",IIF(E.EBL_STATUS="REPROCESSANDO","Tentando integrar","Nao integrar")) AS STATUS,
       E.EBL_DATA AS DATA,
       E.EBL_TENTATIVAS AS TENTATIVAS,
       E.EBL_OBS AS OBS
FROM EBL_ITENS_ERRO E
WHERE [E.EBL_INTEGRACAO = :INTEGRACAO]
      [AND E.EBL_STATUS = :STATUS]
      [AND E.EBL_CODIGO = :CODIGO]
      [AND E.EBL_OBS LIKE :OBS]
ORDER BY E.EBL_CODIGO
</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
</root>