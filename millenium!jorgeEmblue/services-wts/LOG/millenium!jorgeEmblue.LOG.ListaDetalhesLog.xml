<?xml version="1.0"?>
<root>
  <OBJECT NAME="LOG">
    <METHOD NAME="ListaDetalhesLog" DESCRIPTION="Detalhes do log de inetgra&#231;&#227;o" VERSION="14" INTFTYPE="5" THREADSAFE="1">
      <PARAMS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <PARAM NAME="EBL_LOG" FORMAT="N" SIZE="8" FLAGS="2" PROJECTION="0" FIELDLABEL="Integracao" CTAB="0"/>
      </PARAMS>
      <FIELDS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <FIELD NAME="CODIGO" SIZE="60" FLAGS="1" PROJECTION="0" ORDER="1" FIELDLABEL="Codigo" CTAB="0"/>
        <FIELD NAME="OBS" SIZE="250" FLAGS="1" PROJECTION="0" ORDER="3" FIELDLABEL="Obs" CTAB="0"/>
        <FIELD NAME="DATA" FORMAT="H" SIZE="18" FLAGS="1" PROJECTION="0" ORDER="2" FIELDLABEL="Data" CTAB="0"/>
      </FIELDS>
      <ACTIONSCRIPT>SELECT EI.EBL_CODIGO AS CODIGO,
       EI.EBL_OBS AS OBS,
       EI.EBL_DATA AS DATA
FROM EBL_ITENS_LOGS EI
WHERE EI.EBL_LOG=:EBL_LOG
ORDER BY EI.EBL_DATA
</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
</root>