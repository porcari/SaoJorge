<?xml version="1.0"?>
<root>
  <OBJECT NAME="LOG">
    <METHOD NAME="ListarLog" DESCRIPTION="Lod de integra&#231;&#245;es" VERSION="26" INTFTYPE="14" THREADSAFE="1">
      <PARAMS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <PARAM NAME="INTEGRACAO" FORMAT="N" SIZE="8" FLAGS="3" DEFAULT="MILLENIUM!JORGEEMBLUE.INTEGRACOES.LISTAR([],'INTEGRACAO',True)" PROJECTION="0" ORDER="1" LOOKUP="INTEGRACOES.Listar" LOOKUPKEY="INTEGRACAO" LOOKUPDISPLAY="DESCRICAO" LOOKUPCODE="CODIGO" FIELDLABEL="Integra&#231;&#227;o" CTAB="0"/>
        <PARAM NAME="DATA" FORMAT="D" SIZE="10" FLAGS="1" DEFAULT="TODAY" PROJECTION="0" ORDER="2" FIELDLABEL="Data" CTAB="0"/>
      </PARAMS>
      <FIELDS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <FIELD NAME="DESCRICAO" SIZE="60" FLAGS="1" PROJECTION="0" ORDER="4" FIELDLABEL="Descri&#231;&#227;o" CTAB="0"/>
        <FIELD NAME="DATA_INICIO" FORMAT="H" SIZE="18" FLAGS="1" PROJECTION="0" ORDER="5" FIELDLABEL="Data Inicio" CTAB="0"/>
        <FIELD NAME="DATA_FINAL" FORMAT="H" SIZE="18" FLAGS="1" PROJECTION="0" ORDER="6" FIELDLABEL="Data Final" CTAB="0"/>
        <FIELD NAME="REGISTROS_PROCESSADOS" FORMAT="N" SIZE="8" FLAGS="1" PROJECTION="0" ORDER="7" FIELDLABEL="QTD de Registros Processados" CTAB="0"/>
        <FIELD NAME="TRANS_ID" FORMAT="N" SIZE="8" FLAGS="1" PROJECTION="0" ORDER="8" FIELDLABEL="Trans Id Utilizado" CTAB="0"/>
        <FIELD NAME="INTEGRACAO" FORMAT="N" SIZE="8" PROJECTION="0" FIELDLABEL="Integracao" CTAB="0"/>
        <FIELD NAME="STATUS_ULTIMO_PROCESSO" SIZE="150" FLAGS="1" PROJECTION="0" ORDER="9" FIELDLABEL="Status" CTAB="0"/>
        <FIELD NAME="EBL_LOG" FORMAT="N" SIZE="8" FLAGS="2" PROJECTION="0" FIELDLABEL="Ebl Log" CTAB="0"/>
      </FIELDS>
      <ACTIONSCRIPT>SELECT LG.EBL_INTEGRACAO AS INTEGRACAO,
       IT.EBL_DESCRICAO AS DESCRICAO,
       LG.EBL_DATA_INICIO AS DATA_INICIO,
       LG.EBL_DATA_FINAL AS DATA_FINAL,
       #NULL_TO_Z(LG.EBL_REGISTROS_PROCESSADOS) AS REGISTROS_PROCESSADOS,
       LG.EBL_TRANS_ID AS TRANS_ID,
       LG.EBL_STATUS_ULTIMO_PROCESSO AS STATUS_ULTIMO_PROCESSO,
       LG.EBL_LOG
FROM EBL_LOGS LG
INNER JOIN EBL_INTEGRACOES IT ON IT.EBL_INTEGRACAO=LG.EBL_INTEGRACAO
WHERE [LG.EBL_INTEGRACAO=:INTEGRACAO]
      [AND CAST(#MONTH(LG.EBL_DATA_INICIO)||"/"||#DAY(LG.EBL_DATA_INICIO)||"/"||#YEAR(LG.EBL_DATA_INICIO) AS DATE)=:DATA]
ORDER BY LG.EBL_INTEGRACAO,LG.EBL_DATA_INICIO
</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
</root>